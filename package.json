{
  "name": "vue-prop-data-fallback",
  "version": "1.0.1",
  "sideEffects": false,
  "description": "Vue mixin to support an optional prop by falling back to a local data",
  "main": "dist/vue-prop-data-fallback.cjs.js",
  "module": "dist/vue-prop-data-fallback.es.js",
  "unpkg": "dist/vue-prop-data-fallback.js",
  "browser": "dist/vue-prop-data-fallback.es.js",
  "author": {
    "name": "Eduardo San Martin Morote",
    "email": "posva13@gmail.com"
  },
  "scripts": {
    "lint": "eslint --color --ext=js,html src test --fix",
    "unit": "jest",
    "dev": "npm run unit -- --watchAll",
    "test": "npm run lint && npm run unit",
    "prepublishOnly": "rollit"
  },
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "^23.0.1",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.0.2",
    "eslint": "^5.0.0",
    "eslint-config-posva": "^2.0.0",
    "jest": "^23.1.0",
    "vue": "^2.5.17"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/posva/vue-prop-data-fallback.git"
  },
  "bugs": {
    "url": "https://github.com/posva/vue-prop-data-fallback/issues"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/test/*.js",
      "<rootDir>/test/.*.js",
      "<rootDir>/test/*/*.js"
    ]
  },
  "homepage": "https://github.com/posva/vue-prop-data-fallback#readme"
}
